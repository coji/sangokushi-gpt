{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "generate": "run-s generate:openapi generate:client",
    "generate:openapi": "cd api && python -m src.sangokushi_api.commands.generate-openapi-json",
    "generate:client": "bun openapi-typescript api/openapi.json -o app/services/api-client.d.ts",
    "build": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "remix dev",
    "dev:api": "cd api && rye run start",
    "finetune": "cd api && rye run finetune",
    "start": "remix-serve build/index.js",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc",
    "validate": "run-p lint typecheck",
    "clean:files": "tsx scripts/cleanAndConvertFiles"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "@radix-ui/react-avatar": "^1.0.4",
