generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Section {
  id              String   @id @default(uuid())
  volume_title    String
  chapter_number  Int
  chapter_title   String
  section_number  String
  startLineNumber Int
  content         String
  vector          String?
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
