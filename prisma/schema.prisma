// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
}

model section {
  id                String                       @id
  volume_title      String
  chapter_number    Int
  chapter_title     String
  section_number    String
  start_line_number Int
  content           String
}
